{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "richtext"
    },
    "client": {
      "via": "projects",
      "model": "client"
    },
    "short_description": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    }
  }
}
